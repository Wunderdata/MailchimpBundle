{
  "name": "djamadeus/mailchimp-bundle2",
  "type": "symfony-bundle",
  "description": "Symfony2 Bundle for Mailchimp v2.0 API and Export v1.0 API",
  "keywords": ["mailchimp","api","email"],
  "homepage": "https://github.com/MikeRoetgers/MailchimpBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Ladenthin",
      "email": "stefan@wunderdata.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/framework-bundle": ">=3.0",
    "ext-curl": "*",
    "mailchimp/mailchimp": "2.0.*",
    "sensio/buzz-bundle": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
    "psr-0": { "Wunderdata\\MailchimpBundle": "" }
  },
  "target-dir": "Wunderdata/MailchimpBundle"
}
